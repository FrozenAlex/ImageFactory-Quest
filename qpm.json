{
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "ImageFactory",
    "id": "ImageFactory",
    "version": "1.0.2",
    "url": null,
    "additionalData": {
      "overrideSoName": "libImageFactory.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": ["pwsh ./scripts/build.ps1 -clean"],
      "copy": [
        "pwsh ./scripts/copy.ps1 $0:?"
      ],
      "logcat": [
        "pwsh ./scripts/start-logging.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1"
      ]
    },
    "qmodIncludeDirs": ["./build", "./extern/libs"],
    "qmodOutput": "./#{name}.qmod"
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.2",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.4",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.3",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "4007.*",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.54",
      "additionalData": {}
    },
    {
      "id": "config-utils",
      "versionRange": "^2.0.3",
      "additionalData": {}
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.6.4",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "gif-lib",
      "versionRange": "^5.2.2",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "kaleb",
      "versionRange": "^0.1.9",
      "additionalData": {
        "private": true
      }
    }
  ],
  "additionalData": {}
}